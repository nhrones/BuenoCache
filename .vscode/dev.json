{
   "simple": {
      "TargetFolder": "./",
      "Port": 8080
   },
   "hot": {
      "TargetFolder": "./dist",
      "ServeFolder": "./",
      "Port": 8080,
      "HotPort": 9099,
      "BuildTarget": "./dist",
      "Entry": [
         "./src/main.ts"
      ],
      "Watch": "src",
      "Minify": false
   },
   "build": {
      "Out": "dist",
      "FileName": "bundle.js",
      "Entry": [
         "./src/main.ts"
      ],
      "Minify": false
   }
}